#include <iostream>
using namespace std;
int main()
{
int arr[]={12,8,9,14,5,4,65,3,89,42,11,69,56,88,2};
int n=sizeof(arr)/sizeof(arr[0]);

int target=90;


for(int i=0;i<n;i++)
{int position=i;
if(arr[i]+arr[i+1]==target)
	cout<<"Found "<<position<<" and "<<position+1;

}






return 0;
}

