#include <iostream>
using namespace std;

void main()
{
int n;
int arr[n];
cout<<"Enter the number of element in the array";
cin>>n;
cout<<"Enter the elements";
for(int i=0;i<n;i++)
cin>>arr[i];
for(int i=0;i<n;i++)
cout<<arr[i];

}



