#include <iostream>
using namespace std;

struct Node{
	int data;
	Node *leftChild;
	Node *rightChild;
};

struct Node Insert( Node *node,int data)
{
Node *tempNode=new Node;

tempNode->data=data;
tempNode->leftChild=NULL;
tempNode->rightChild=NULL;
if(node==NULL)
{node=tempNode;
 return node;	}
else
   if(data < node->data)
	    return Insert(node->leftChild,data);
   else
	   return Insert(node->rightChild,data);


}



int main()
{
 Node *root=NULL;
root=Insert(root,15);

return 0;
}

